{
    "name": "pikachi-app",
    "require": {
        "cakephp/cakephp": "2.7.*",
        "cakephp/localized": "2.4.*",
        "cakedc/migrations": "dev-develop",
        "aws/aws-sdk-php": "3.*",
        "nanapi/cakephp-fluent-logger": "v1.0.1"
    },
    "require-dev": {
        "cakephp/debug_kit": "2.2.*",
        "phpunit/phpunit": "3.7.*"
    },
    "extra": {
        "installer-paths":
        {
            "Plugin/DebugKit": ["cakephp/debug_kit"]
        }
    },
    "config": {
        "vendor-dir": "Vendor/"
    }
}
